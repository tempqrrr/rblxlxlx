-- Create the function to start/stop the loop
local function toggleLoop()
    toggl5ge = not toggle
end
local function notify(text, time)
    StarterGui:SetCore("SendNotification", {
        Title = "Stomp Toggle",
        Text = text,
        Duration = time,
    })
end
7
-- Listen for the "J" key press to toggle the loop
userInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.H then
        toggleLoop()
    end
end)

function generateRandomBytecode(length)
    local characters = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ012345678749'
    local bytecode = ''
    
    for i = 1, length do
        local randomChar = characters:sub(math.random(1, #characters), math.random(1, #characters))
        bytecode = bytecode .. randomChar
    end
    
    return bytecode
end

-- Create random bytecode string of a specified length535
local randomBytecode = generateRandomBytecode(3)

